module('perftestsjs.tests').houghcircle = (function() {

	var canvas;
	var context;

	function startFN() {
		canvas = $('#HoughCirclePage #canvas').get()[0];
		context = canvas.getContext('2d');
		
		var img = new Image();
		img.src = 'images/balls.jpg'; 
		
		img.onload = function(){
		    context.drawImage(img, 0, 0);
	        StopWatch w = new StopWatch();
			var image = context.createImageData(canvas.offsetWidth, canvas.offsetHeight);
			var imageData = image.data;
	        log(w.stop());
	        
		    
//			var image = context.createImageData(mandelbrotSide, mandelbrotSide);
//			var imageData = image.data;
		};		
	}

	function stopFN() {
	}

	return {
		start : startFN,
		stop : stopFN,
		name : "HoughCircle"
	}

})();